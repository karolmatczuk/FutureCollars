-- 4.
SELECT *
FROM pracownicy
WHERE Imie = 'Anna';

-- 5.
SELECT *
FROM pracownicy
WHERE Imie IS NULL;

-- 6. 
SELECT Kurs
FROM pracownicy
WHERE wiek BETWEEN 30 AND 40;

-- 7. 
SELECT wiek
FROM pracownicy
WHERE ID BETWEEN 1 AND 7;

-- 8. 
SELECT *
FROM pracownicy
WHERE Wiek IS NULL;

-- 9. 
ALTER TABLE pracownicy
RENAME COLUMN Kurs TO Szkolenie;
